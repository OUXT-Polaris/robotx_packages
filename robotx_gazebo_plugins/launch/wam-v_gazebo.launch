<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
    args="-urdf -model wam-v -param robot_description -x 0 -y 0 -z -0.3"/>

  <include file="$(find robotx_gazebo_plugins)/launch/spawn_environment_model.launch"/>
  <include file="$(find robotx_visualization)/launch/visualization.launch"/>
  <include file="$(find robotx_description)/launch/wam-v.launch"/>
  <include file="$(find robotx_navigation)/launch/navigation.launch"/>
  <include file="$(find robotx_recognition)/launch/recognition.launch"/>

</launch>
